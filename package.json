{
  "name": "eager-self-summary",
  "version": "0.0.0",
  "description": "A self summary built with [Eager](https://eager.io).",
  "main": "app.js",
  "scripts": {
    "watch:js": "genie-smoke",
    "watch:css": "postcss --watch --use autoprefixer -o build/app.css app.css",
    "build": "NODE_ENV=production babel --out-dir build app.js && postcss --use autoprefixer -o build/app.css app.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EagerIO/SelfSummary.git"
  },
  "author": "Teffen Ellis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EagerIO/SelfSummary/issues"
  },
  "homepage": "https://github.com/EagerIO/SelfSummary#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-eslint": "6.0.4",
    "eslint": "2.9.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "genie-smoke": "0.0.0",
    "postcss-cli": "2.5.2"
  }
}
